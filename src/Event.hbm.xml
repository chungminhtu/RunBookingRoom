<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
   	 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.runsystem.booking.controller.Event" table="events">
		<id column="event_id" name="id" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property column="start_date" name="start_date" type="timestamp" />
		<property column="end_date" name="end_date" type="timestamp" />
		<property column="contacts" name="contacts" type="java.lang.String" />
		<property column="notes" name="notes" type="java.lang.String" />
		<property column="tablename" name="table" type="java.lang.String" />

	</class>
</hibernate-mapping>
